<div id="gameUpdateModal" class="ui modal">
    <i class="close icon"></i>
    <div class="header">
        Please provide the updated File ID
    </div>
    <div class="image content">
        <div class="ui medium image">
            <!-- path to image file -->
            <img src="/images/avatar/large/chris.jpg">
        </div>
        <div class="description">
            <h2 class="ui header"></h2>
            <form id="gameUpdateForm" class="ui form" action="/user/update" method="POST">
                <div class="field">
                    <label>New File Id</label>
                    <input type="text" name="newFileId">
                </div>
                <input type="text" name="gameToUpdateId" type="hidden" id="gameToUpdateId">
                
            </form>
        </div>
    </div>
    <div class="actions">
        <div class="ui black deny button">
            Cancel
        </div>
        <div class="ui positive approve right labeled icon button">
            Ready to Update!
            <i class="checkmark icon"></i>
        </div>
    </div>
</div>